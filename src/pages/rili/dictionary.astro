---
import BaseLayout from "@/layouts/BaseLayout.astro";
import TitleScrollableTable from "@/components/TitleScrollableTable";

const tableData = [
  ["El otro", "-->", "Al-el-varo"],
  ["El otro otro", "-->", "Island Stork"],
  ["El otro otro otro", "-->", "Al-el-no-tan-varo"],
  ["El otro otro otro otro", "-->", "Al-el-aun-menos-varo"],
];
---

<BaseLayout
  title="Rili Dictionary"
  description="Dictionary for knowing what rili words mean"
>
  <TitleScrollableTable
    title="Diccionario Rili"
    tableData={tableData}
    tableHead={["Rili Word", "", "Word"]}
    client:load
  />
</BaseLayout>
