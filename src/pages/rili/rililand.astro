---
import BaseLayout from "@/layouts/BaseLayout.astro";
import RiliLand from "@/components/Rili/RiliLand";
import { Image } from "astro:assets";
import RiliLandLogo from "@/assets/RililandLogoNoBg.png";
---

<BaseLayout
  title="RiliLand"
  description="The riliest place on uranus"
  image="/RililandLogoNoBg.png"
>
  <RiliLand client:only="react">
    <Image
      src={RiliLandLogo}
      alt="RiliLand logo"
      draggable="false"
      height="196"
    />
  </RiliLand>
</BaseLayout>
