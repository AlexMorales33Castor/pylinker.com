{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "preserve",
    "target": "esnext",
    "module": "esnext",
    "baseUrl": ".",
    "paths": {
      "@c/*": ["src/components/*"],
      "@l/*": ["src/layouts/*"],
      "@s/*": ["src/styles/*"]
    }
  },
}
